[Documentation]
Name        = Bruteforce predictables discovery
Description = Tries to discover hidden files at predictable locations.
              For example: (Apache) www.site.com/error_log
Author      = GoLismero project team
Version     = 0.1
Website     = https://github.com/golismero
Copyright   = Copyright (C) 2011-2014 GoLismero Project
License     = GNU Public License v2

[Core]
Module = brute_url.py
Class = PredictablesDisclosureBruteforcer

[Arguments]
server_name =

[common]
w2 = fuzzdb/Discovery/PredictableRes/Logins.fuzz.txt
w3 = fuzzdb/Discovery/PredictableRes/Passwords.fuzz.txt
w4 = fuzzdb/Discovery/PredictableRes/proxy-conf.fuzz.txt
w5 = fuzzdb/Discovery/PredictableRes/Randomfiles.fuzz.txt

#
# Predictables by Web server
#

