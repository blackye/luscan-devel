[Documentation]
Name        = Bruteforce file extensions discovery
Description = Tries to discover hidden files by brute force:
              www.site.com/index.php -> www.site.com/index.php.old
Author      = GoLismero project team
Version     = 0.1
Website     = https://github.com/golismero
Copyright   = Copyright (C) 2011-2014 GoLismero Project
License     = GNU Public License v2

[Core]
Module = brute_url.py
class = FileExtensionsDisclosureBruteforcer

#
# Extensions by language
#

[common_extensions]
w1 = golismero/Discovery/FilenameBruteforce/file-compressed-extensions.txt
w2 = fuzzdb/Discovery/FilenameBruteforce/Extensions.Backup.fuzz.txt
w2 = golismero/Discovery/FilenameBruteforce/file-extensions.txt

[microsoft_extensions]
w1 = golismero/Discovery/FilenameBruteforce/microsoft-file-extensions.txt

[java_extensions]
w1 = golismero/Discovery/FilenameBruteforce/java-file-extensions.txt

[php_prefixes]
w1 = golismero/Discovery/FilenameBruteforce/file-prefix.txt
