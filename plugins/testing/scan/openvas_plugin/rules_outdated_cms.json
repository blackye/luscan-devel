[

    {
        "comment": "--- WordPress advisories ---",

        "rule_id" : "cms1",
        "rule_type" : "software",
        "matching_types":
            [
                "OutdatedSoftwareWordPress"
            ],
        "filename_rules":
            [
                [
                    {
                        "regex": "(^(gb|secpod)_wordpress_.*\\.nasl)"
                    },
                    {
                        "regex": "(^gb_wordpressmu_.*\\.nasl)"
                    },
                    {
                        "regex": "(^[Ww]ord[Pp]ress_.*\\.nasl)"
                    },
                    {
                        "regex": "(.*_detect\\.nasl)",
                        "negate": "true"
                    }
                ]
            ]
    },

